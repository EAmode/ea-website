  {
  "compilerOptions": {
    "target": "es2018",
   
    "moduleResolution": "node",
    "noEmitOnError": false,
    "lib": ["es2017", "dom"],
    "strict": false,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "outDir": "out-tsc",
    "sourceMap": true,
    "inlineSources": true,
    "rootDir": "./",
    "strictNullChecks": false
  },
  "include": ["**/*.ts"],
  "exclude": ["src.old"]
}
